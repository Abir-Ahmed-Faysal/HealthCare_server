model MedicalReport {
    id         String   @id @default(uuid(7))
    patientId  String   
    patient    Patient  @relation(fields: [patientId], references: [id], onDelete: Cascade, onUpdate: Cascade)
    reportName String
    reportLink String
    createdAt  DateTime @default(now())
    updatedAt  DateTime @updatedAt

    @@index([patientId])
    @@map("MedicalReport")
}
